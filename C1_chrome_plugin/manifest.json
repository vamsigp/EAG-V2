{
  "manifest_version": 3,
  "name": "URL Redirects & Cookies & Analytics Analyzer",
  "description": "Analyzes redirects, cookies, bandwidth, IP, CNAME, TLS info and detects analytics providers on the current page.",
  "version": "1.3",
  "permissions": [
    "cookies",
    "webRequest",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Analyze Page"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_idle"
    }
  ]
}